
#构建平台，是 DOCKER_SWARM,KUBERNETES
BUILD_PLATFORM="DOCKER_SWARM"

BUILD_DOCKER_STACK_NAME="unsun"

BUILD_DOCKER_SWARM_NETWORK="jshyun_overlay_network"

#配置harbor仓库地址
BUILD_HARBOR_ADDRESS="harbor.jsy.com"

#配置harbor仓库
BUILD_HARBOR_PROJECT="unsun"

#启用dockerfile,路由dockerfile
BUILD_ENABEL_DOCKERFILES="jpgcoll-engine"

#启用模板
BUILD_ENABEL_TEMPLATES="console-zuul,jshyun-zuul,mall-zuul,task-scheduler,middleware-zuul,entrance-management"
